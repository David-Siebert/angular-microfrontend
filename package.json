{
  "name": "angular-microfrontend",
  "version": "1.0.0",
  "scripts": {
    "initMainUI": "cd .\\MainUI\\ && npm install",
    "initMicroAppA": "cd .\\MicroAppA\\ && npm install",
    "initMicroAppB": "cd .\\MicroAppB\\ && npm install",
    "initMicroAppC": "cd .\\MicroAppC\\ && npm install",
    "init": "npm run initMainUI && npm run initMicroAppA && npm run initMicroAppB && npm run initMicroAppC",
    "Main": "cd .\\MainUI\\ && npm run start",
    "AppA": "cd .\\MicroAppA\\ && npm run start",
    "AppB": "cd .\\MicroAppB\\ && npm run start",
    "AppC": "cd .\\MicroAppC\\ && npm run start"
  }  
}
